{
    "application": "application.xml",
    "config": "air",
    "type": "app",
    "compilerOptions": {
        "output": "bin/xenon.swf",
        "library-path": [
            "src/assets/"
        ],
        "define": [
            {
                "name": "CONFIG::debug",
                "value": false
            },
            {
                "name": "CONFIG::release",
                "value": true
            },
            {
                "name": "CONFIG::timeStamp",
                "value": "\"2020-06-30\""
            }
        ],
        "default-size": {
            "width": 1280,
            "height": 800
        },
        "default-frame-rate": 60,
        "default-background-color": "#000000",
        "debug": false,
        "swf-version": 43
    },
    "files": [
        "src/Main.as"
    ],
    "airOptions": {
        "windows": {
            "output": "bin/xenon",
            "signingOptions": {
                "storetype": "pkcs12",
                "keystore": "bat/XenonKey.p12"
            }
        },
        "mac": {
            "output": "bin/xenon.air",
            "signingOptions": {
                "storetype": "pkcs12",
                "keystore": "bat/XenonKey.p12"
            }
        },
        "files": [
            {
                "file": "bin",
                "path": "bin"
            }
        ]
    }
}